<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.booklibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/library"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AlreadyReadBook"
            android:label="Already Read Books"></activity>
        <activity android:name=".About"
            android:label="About"/>
        <activity android:name=".WantToReadBook"
           android:label="Want To Read Books"/>
        <activity android:name=".CurrentlyReadingBooks"
            android:label="Currently Reading"/>
        <activity android:name=".BookActivity"
            android:label="Book Activity"/>
        <activity android:name=".AllBooksActivity"
            android:label="All Books"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>